[Context]
sockets=!ssh-auth;!pulseaudio;wayland;!x11
devices=!all;dri
features=!devel
filesystems=!xdg-config/kdeglobals;xdg-config/git:ro;~/Dev;!host
persistent=.npm;.vscode-cli;.apklab;.vscode;.dotnet;.pki

[Environment]
PATH=/app/bin:/usr/bin:/usr/lib/sdk/node18/bin/:/usr/lib/sdk/openjdk/bin

[System Bus Policy]
org.freedesktop.login1=none

[Session Bus Policy]
org.freedesktop.Flatpak=none
